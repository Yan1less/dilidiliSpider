<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yan1less.mapper.AnimateMapper">

    <insert id="InsertDatas" parameterType="Animate">
    insert into Animate values(#{animate_id}, #{link}, #{place}, #{img}, #{tittle}, #{date}, #{tag}, #{characterVoice}, #{madeFrom}, #{watchPoint}, #{introduce})
  </insert>

</mapper>